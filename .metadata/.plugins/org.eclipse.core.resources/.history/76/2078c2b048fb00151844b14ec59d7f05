package Casting;

public class Main {

	public static void main(String[] args) {
		partOne();
	}
	
	private static void partOne() {

		float fValue = 32.5f;
		int iValue = (int) fValue;
		
		// prints 32
		System.out.println("Number Casting: " + iValue);
		
		iValue = Math.round(fValue);
		
		// prints 33
		System.out.println("Math rounding: " + iValue);
		
	}

}
