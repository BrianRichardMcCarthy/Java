package Casting;

public class ClassCasting {

	public static void main(String[] args) {
		
		Person p1 = new Person();
		Enemy e1 = new Enemy();

	}

}


class Person {
	public void speak() { System.out.print("Hello Citizien"); }
}

class Enemy extends Person {
	public void speak() { System.out.print("Hand your wallet"); }
	public void surrender() { System.out.print("I Yield"); }
}